{
  "name": "museum-solo-ai",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:sandbox": "wrangler pages dev dist --ip 0.0.0.0 --port 3000 --d1=museum-solo-ai-production --local",
    "build": "vite build",
    "preview": "wrangler pages dev",
    "deploy": "npm run build && wrangler pages deploy dist --project-name museum-solo-ai",
    "deploy:prod": "npm run build && wrangler pages deploy dist --project-name museum-solo-ai --branch main",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "db:migrate:local": "wrangler d1 migrations apply museum-solo-ai-production --local",
    "db:migrate:prod": "wrangler d1 migrations apply museum-solo-ai-production",
    "clean-port": "fuser -k 3000/tcp 2>/dev/null || true",
    "test": "curl http://localhost:3000/health"
  },
  "dependencies": {
    "@google-cloud/text-to-speech": "^6.4.0",
    "@google-cloud/vision": "^5.3.4",
    "@google/generative-ai": "^0.24.1",
    "@modelcontextprotocol/sdk": "^1.23.0",
    "googleapis": "^166.0.0",
    "hono": "^4.10.7"
  },
  "devDependencies": {
    "@hono/vite-build": "^1.2.0",
    "@hono/vite-dev-server": "^0.18.2",
    "vite": "^6.3.5",
    "wrangler": "^4.4.0"
  }
}
